{
  "bionic": {
    "containers": [
      {
        "name": "builder",
        "image": "docker.rtbrick.net/devops/images/rtbrick/ubuntu-bionic-builder:latest",
        "compile_deps": [
          "libarchive-dev",
          "libbz2-dev",
          "liblz-dev",
          "zlib1g-dev",
          "libzstd-dev"
        ],
        "build_commands": [
          "./configure",
          "make"
        ],
        "gitlab_repos": []
      }
    ],
    "pkg_commands": [
      "make install"
    ],
    "pkg_dev_commands": [],
    "pkg_dbg_commands": []
  },
  "__defaults__": {
    "project": "bgpdump2",
    "version_mmr": {
      "major": 2,
      "minor": 0,
      "rev": 9,
      "label": "",
      "meta": ""
    },
    "pkg_name": "rtbrick-bgpdump2",
    "pkg_descr": "RtBrick fork of bgpdump2 - github.com/yasuhiro-ohara-ntt/bgpdump2",
    "pkg_group": "rtbrick-internal",
    "pkg_deps": [
      "libarchive13",
      "libbz2-1.0",
      "liblz1",
      "zlib1g",
      "libzstd1"
    ],
    "pkg_services": [],
    "upload_script": ".jenkins/jenkins_upload_to_latest_ubuntu_rtbrick-internal_bionic_rtbrick-internal.sh"
  }
}
